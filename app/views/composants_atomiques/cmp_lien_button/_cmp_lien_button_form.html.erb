<div class="formulaire cmp" id="<%=obj.reference%>">
 
 <h2 class="titre">
    <%=t("CmpComposant.CmpLienButton.titre")%>
 </h2>

 <table class="tformulaire">
  <tr>
    <td><label><%=t("nom")%> </label></td>
    <td><input id="<%=obj.reference%>_nom" type="text" value="<%=obj.nom%>" name="<%=obj.reference%>[nom]"></input></td>
  </tr>
  <tr>
    <td><label><%=t("lien")%> </label></td>
    <td><input id="<%=obj.reference%>_texte" type="text" value="<%=obj.texte%>" name="<%=obj.reference%>[texte]"></input></td>
  </tr>
  <tr>
    <td><label><%=t("width")%> </label></td>
    <td><input id="<%=obj.reference%>_width" type="text" value="<%=(obj.width== 0)?"":obj.width%>" name="<%=obj.reference%>[width]"></input></td>
  </tr>
  <tr>
    <td><label><%=t("height")%> </label></td>
    <td><input id="<%=obj.reference%>_height" type="text" value="<%=(obj.height== 0)?"":obj.height%>" name="<%=obj.reference%>[height]"></input></td>
  </tr>
  <tr>
    <td><label><%=t("titre")%> </label></td>
    <td><input id="<%=obj.reference%>_titre" type="text" value="<%=obj.titre%>" name="<%=obj.reference%>[titre]"></input></td>
  </tr>
</table>
</div>