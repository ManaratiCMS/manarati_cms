<input type="hidden"  name="id"  value="<%=@session_formation.id%>"></input>
<table class="formulaire">
   <tr>
    <td><label><%=t("GestionSessionFormationG2.Label.formation")%> </label></td>
    <td><%=select("session_formation","formation_g2_id",@formations.collect {|l| [ l.titre, l.id ] }, { :include_blank => true })%></td>
  </tr>
  <tr>
     <td><label><%=t("GestionSessionFormationG2.Label.Dates_Debut_Inscription")%> </label></td>
    <td><%= f.text_field :Dates_Debut_Inscription ,:class=>"validate[required] text-input" %></td>
  </tr>
 <tr>
     <td><label><%=t("GestionSessionFormationG2.Label.Date_Fin_Inscription")%> </label></td>
    <td><%= f.text_field :Date_Fin_Inscription ,:class=>"validate[required] text-input" %></td>
  </tr>
  <tr>
    <td><label><%=t("GestionSessionFormationG2.Label.local")%></label></td>
     <td><%=select("session_formation","local_formation_g2_id",@locaux.collect {|l| [ l.nom, l.id ] }, { :include_blank => true })%></td>

  </tr>
   

</table>