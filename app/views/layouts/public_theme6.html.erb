                <div class="left_area" >
                <%=yield :left_area%>
                </div>
                <%-end-%>

                <%= yield %>
               
                <%- if instance_variable_defined?('@content_for_zone_droit')   -%>
                <div id="zone_droit">
                <%= yield :zone_droit %>
